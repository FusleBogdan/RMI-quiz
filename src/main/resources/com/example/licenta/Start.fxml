<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #FFDE59;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.licenta.StartController">
   <children>
      <ImageView fitHeight="169.0" fitWidth="218.0" layoutX="216.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../img/logo.png" />
         </image>
      </ImageView>
      <Button fx:id="signupbutton" layoutX="216.0" layoutY="212.0" mnemonicParsing="false" onAction="#handleSignUpButtonAction" prefHeight="45.0" prefWidth="169.0" style="-fx-background-color: #38B6FF;" text="Sign Up" textFill="WHITE">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Button>
      <Button fx:id="loginbutton" layoutX="216.0" layoutY="287.0" mnemonicParsing="false" onAction="#handleLogInButtonAction" prefHeight="45.0" prefWidth="169.0" style="-fx-background-color: #38B6FF;" text="Log In" textFill="WHITE">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
